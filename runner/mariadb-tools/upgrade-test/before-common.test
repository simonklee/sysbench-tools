	INOUT param4 INT , INOUT param5 INT , INOUT param6 INT, INOUT param7 INT , INOUT param8 INT ,
	INOUT param9 INT , INOUT param10 INT , INOUT param11 INT, INOUT param12 INT , INOUT param13 INT
)
LANGUAGE SQL
